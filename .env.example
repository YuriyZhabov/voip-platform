# Основные настройки LiveKit Cloud
LIVEKIT_DOMAIN=your-domain.com
LIVEKIT_PUBLIC_IP=your.public.ip.address
LIVEKIT_URL=wss://your-livekit-instance.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# Настройки Novofon SIP
NOVOFON_NUMBER=+7XXXXXXXXXX
NOVOFON_SIP_SERVER=sip.novofon.ru
NOVOFON_SIP_PORT=5060
NOVOFON_USERNAME=your_novofon_username
NOVOFON_PASSWORD=your_novofon_password

# Cloud API (дополнительные ключи)
CLOUD_API_KEY=your_cloud_api_key
CLOUD_API_SECRET=your_cloud_api_secret

# AI сервисы
OPENAI_API_KEY=sk-proj-your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini
OPENAI_TEMPERATURE=0.7

DEEPGRAM_API_KEY=your_deepgram_api_key
DEEPGRAM_MODEL=nova-3
DEEPGRAM_LANGUAGE=ru

CARTESIA_API_KEY=sk_car_your_cartesia_api_key
CARTESIA_MODEL=sonic-multilingual
CARTESIA_VOICE=your_cartesia_voice_id
CARTESIA_LANGUAGE=ru

# Asterisk настройки
ASTERISK_USER=asterisk
ASTERISK_MANAGER_SECRET=your_manager_secret

# Timezone
TZ=Europe/Moscow